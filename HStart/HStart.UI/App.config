<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="StartProcInfoConfiguration" type="HStart.Configuration.StartProcInfoSection, HStart.Configuration, Version=1.0.0.30674, Culture=neutral, PublicKeyToken=null" />
  </configSections>
  <StartProcInfoConfiguration>
    <StartProcInfos>
      <addProcInfo key="coinotron PPC W"
                   CreateNoWindow="false"
                   FileName="D:\Bitcoin-miner\cgminer\cgminer.exe"
                   Arguments="-c coinotron-ppc-IntelHDG4000.conf --net-delay --sharelog cgminer-share.log"
                   ProcessWindowStyle="Minimized" 
                   WorkingDirectory="D:\Bitcoin-miner\cgminer"/>
      <addProcInfo key="coinotron PPC H"
                   CreateNoWindow="false"
                   FileName="D:\Bitcoin-miner\cgminer\cgminer.exe"
                   Arguments="--lookup-gap 2 --shaders 112 --worksize 128 --intensity 4 -g 1 -o http://coinotron.com:8322 -u Cameleer.1 -p helicopter --sharelog cgminer-share.log"
                   ProcessWindowStyle="Minimized"
                   WorkingDirectory="D:\Bitcoin-miner\cgminer"/>
    </StartProcInfos>
  </StartProcInfoConfiguration>
  <log4net>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="hstart.ui.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maximumFileSize value="10MB" />
      <maxSizeRollBackups value="10" />
      <staticLogFileName value="false" />
      <immediateFlush value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <!--<conversionPattern value="%date [%4thread] %-5level %logger - %message%newline%exception%newline" />-->
        <conversionPattern value="%date [%4thread] %-5level %logger - %message%newline%exception" />
      </layout>
    </appender>
    <logger name="UserLog">
      <level value="ALL" />
      <appender-ref ref="FileAppender" />
    </logger>
    <logger name="DeveloperLog">
      <appender-ref ref="FileAppender" />
      <level value="ALL" />
    </logger>
  </log4net>
  <appSettings>
    <add key="log4net" value="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </appSettings>
  <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>